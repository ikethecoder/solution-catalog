{
  "services": [],
  "resources": [
    {
      "service_discovery_service": {
        "{{instanceId}}-ide": {
          "Name": "ide",
          "Address": "{{instance.privateIp}}",
          "Port": 2181,
          "Tags" : [ "{{instanceId}}", "{{environmentId}}" ],
          "Check": {
            "url": "http://{{instance.privateIp}}:8000",
            "interval": "10s",
            "timeout": "1s"
          }
        }
      }
    },
    {
      "proxy_service": {
        "play-ide": {
          "environment": "cd",
          "serverNumber": 1,
          "segment": "ci",
          "privateKey": "master_key",
          "proxy_ssl_virtual_host": "default",
          "location": "/play-ide/",
          "template": "ide.tmpl",
          "passthrough": "https://play-ide.service.dc1.consul:8000"
        }
      }
    }
  ]
}
{
  "resources": [
    {
      "service_endpoint": {
        "${instanceId}-{{name}}": {
          "service": "{{name}}",
          "segment": "${segmentId}",
          "url": "http://${instance.privateIp}:{{port}}"
        }
      }
    },
    {
      "service_discovery_service": {
        "${instanceId}-{{name}}-{{port}}": {
          "Name" : "{{name}}",
          "Address" : "${instance.privateIp}",
          "Port" : {{port}},
          "Tags" : [
            "urlprefix-/www strip=/www"
          ],
          "Check" : {
            "http": "http://${instance.privateIp}:{{port}}",
            "interval": "10s",
            "timeout": "1s"
          }
        }
      }
    }
  ]
}

{
  "resources": [
    {
      "service_endpoint": {
        "${instanceId}-{{name}}": {
          "service": "{{name}}",
          "description": "",
          "environment": "${environmentId}",
          "segment": "${segmentId}",
          "url": "http://${domainName}:${port}",
          "type": "console"
        }
      }
    },
    {
      "service_discovery_service": {
        "${instanceId}-{{name}}-${port}": {
          "Name" : "{{name}}",
          "Address" : "${d}{var.compute_instance.${instanceName}.privateIp}",
          "Port" : ${port},
          "Tags" : [
            "urlprefix-/{{name}} strip=/{{name}}"
          ],
          "Check" : {
            "http": "http://${d}{var.compute_instance.${instanceName}.privateIp}:${port}",
            "interval": "10s",
            "timeout": "1s"
          }
        }
      }
    }
  ]
}

{
  "type": "exec",
  "attributes": {
    "run_if": [
      "passed"
    ],
    "on_cancel": null,
    "command": "canzea",
    "arguments": [
      "--reset",
      "--lifecycle=wire",
      "--solution={{solution}}",
      "--action={{action}}",
      "--args={{{parameters}}}"
    ],
    "working_directory": "{{project}}"
  }
}

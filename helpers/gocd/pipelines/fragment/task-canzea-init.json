{
  "type": "exec",
  "attributes": {
    "run_if": [
      "passed"
    ],
    "on_cancel": null,
    "command": "canzea",
    "arguments": [
      "--init",
      "--serverBase={{base}}",
      "--serverNumber={{serverNumber}}",
      "--privateKey=/var/go/.ssh/id_rsa_digitalocean"
    ],
    "working_directory": ""
  }
}
